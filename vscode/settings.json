         "after": ["{"]
     },
     {
         "before": ["Ä"],
         "after": ["["]
     },

    ],
    "vim.handleKeys":{
        "<C-c>": false
    },
    "editor.renderWhitespace": "all",
    "workbench.sideBar.location": "left",
    "editor.minimap.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
